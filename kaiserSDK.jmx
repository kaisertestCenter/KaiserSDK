          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import Encode.MD5;
import Encode.SignSort;
import com.alibaba.fastjson.JSONObject;
n = vars.get(&quot;n&quot;);
username = &quot;testmaster000&quot;+n;
vars.put(&quot;username&quot;,username);
password = &quot;123456&quot;+n;
password = MD5.getMd5(password);
vars.put(&quot;password&quot;,password);
time = System.currentTimeMillis()+&quot;&quot;;
//时间戳捡去后三位
time = time.substring(0,time.length()-3);
log.info(time);
vars.put(&quot;time&quot;,time);
uuid = &quot;test&quot;+n;
AppKey = &quot;4a6a42408c74276b78c61f9141ba3b49&quot;;
JSONObject params = new JSONObject();
params.put(&quot;device&quot;,n);
params.put(&quot;password&quot;,password);
params.put(&quot;adchal&quot;,&quot;99999&quot;);
params.put(&quot;msi&quot;,&quot;123&quot;);
params.put(&quot;time&quot;,time);
params.put(&quot;androidID&quot;,&quot;1&quot;);
params.put(&quot;ver_code&quot;,&quot;15005&quot;);
params.put(&quot;uuid&quot;,uuid);
params.put(&quot;mac&quot;,&quot;123&quot;);
params.put(&quot;appid&quot;,&quot;100000020&quot;);
params.put(&quot;net&quot;,&quot;wifi&quot;);
params.put(&quot;sv&quot;,&quot;6.0&quot;);
params.put(&quot;platform&quot;,&quot;1&quot;);
params.put(&quot;api_version&quot;,&quot;1.2.0&quot;);
params.put(&quot;username&quot;,username);
params.put(&quot;imei&quot;,&quot;12345678&quot;);
params.put(&quot;ip&quot;,&quot;&quot;);
params.put(&quot;ver_name&quot;,&quot;1.5.0&quot;);
params.put(&quot;br&quot;,n);
params.put(&quot;channel&quot;,&quot;100000&quot;);
//排序
param = SignSort.parseMapToString(params,false);
//拼接
sign = param +&quot;&amp;4a6a42408c74276b78c61f9141ba3b49&quot;;
param = param +&quot;&amp;&quot;;
log.info(sign);
//加密
sign = MD5.getMd5(sign);
vars.put(&quot;sign&quot;,sign);
param = &quot;sign=&quot;+sign+&quot;&amp;&quot;+param;
log.info(param);
vars.put(&quot;param&quot;,param);
</stringProp>
          </BeanShellPreProcessor>
